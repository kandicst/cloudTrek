                </v-list-item-content>\r\n                <br>\r\n            </v-list-item>\r\n        </v-list>\r\n    </v-navigation-drawer >\r\n\r\n    </nav>\r\n\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return{\r\n            drawer: false,\r\n\r\n            // linkovi za prelaz na druge delove str\r\n            links : [\r\n                {icon: 'mdi-view-dashboard', text: 'Dashboard', route: '/'},\r\n                {icon: 'mdi-folder', text: 'Folder', route: '/'},\r\n                {icon: 'mdi-account-minus', text: 'Team', route: '/'}\r\n            ]\r\n        }\r\n    },\r\n\r\n    methods : {\r\n        // menja stanje drawera na svaki klik\r\n        toggleDrawer : function() {\r\n            this.drawer = !this.drawer;\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n\r\n<style >\r\n\r\n    .naslov {\r\n        margin-left : 20px;\r\n        color: azure;\r\n        font-size: 35px;\r\n    }\r\n\r\n    .d {\r\n        background-color: red;\r\n    }\r\n\r\n</style>"]}]}